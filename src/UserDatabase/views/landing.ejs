 <!--  include another view file (partial) inside a .ejs view file -->
  <%- include header.ejs %>

  <body class="landing_body">
      <h1 id="landing_header">RETTIWT</h1>
      <!-- Signin Form -->
      <div id="sign_in_div">
        <form id="sign_in_form" action="/post-signin" method="post">
          <h2>Sign In</h2>
          <input type="text" id="usr_email" name="email" placeholder="Email Address" required>
          <input type="text" id="usr_password" name="password" placeholder="password" required>
          <input type="submit" id="sign_in_user" value="Sign In">
        </form>
          <a href="/register" id = "register_link">Register Now!</a>
      </div>


  </body>
</html>
